public int matchUp(int[] nums1, int[] nums2) {
  int count=0;
  
  for(int i=0;i<=nums1.length-1;i++){
    for(int j=0;j<=nums2.length-1;j++){
      
      int diff=nums1[i]-nums2[j];
      if(i==j){
         if(diff<=2&&diff>=-2&&diff!=0){
           count++;
         }
         
          
         
      }
    }
  }  
  
  return count;
}
