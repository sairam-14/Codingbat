public boolean has77(int[] nums) {
  
  for(int i=0;i<nums.length-1;i++){
    for(int j=i+1;j<nums.length;j++){
      if(!(i==nums.length-2)){
      if(nums[i]==7&&nums[j]==7){
        return true;
      } 
        
    if(nums[i]==7&&nums[j+1]==7){
      return true;
    
    }
    else{
      break;
    }
      }
      if(i==nums.length-2){
       if(nums[i]==7&&nums[j]==7){
         return true;
      }
      }
    }
  }
    
  
    return false;
   
}
